-3 Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.
UPDATE employee 
SET name = 'Kerem Uslu' 
WHERE name LIKE 'A%';
----
UPDATE employee 
SET email = 'xxxxx@gmail.com' 
WHERE id>12;
----
UPDATE employee
SET birthday = '2002-10-23'
WHERE name = 'Kerem Uslu';
----
UPDATE employee
SET email = 'uslukerem006@gmail.com'
WHERE birthday = '2002-10-23;
----
UPDATE employee
SET name = 'Merve Turmuş' 
WHERE id>12;
-4 Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee
WHERE NOT name = 'Kerem Uslu' or name = 'Merve Turmuş';
----
DELETE FROM employee
WHERE birthday = '2002-10-23';
----
DELETE FROM employee
WHERE id<20;
----
DELETE FROM employee
WHERE email = 'uslukerem006@gmail.com';
----
DELETE FROM employee
WHERE name LIKE 'C%';

